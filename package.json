{
	"name": "study-lenses-wc-kit",
	"version": "1.0.0",
	"description": "Web components kit and programmatic library for building dynamic code study environments",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./register": {
			"types": "./dist/register.d.ts",
			"import": "./dist/register.js",
			"default": "./dist/register.js"
		},
		"./types": {
			"types": "./dist/types.d.ts",
			"import": "./dist/types.js",
			"default": "./dist/types.js"
		}
	},
	"files": [
		"dist/**/*",
		"README.md",
		"CONTRIBUTING.md",
		"LICENSE"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "vitest",
		"test:run": "vitest run",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest --coverage",
		"test:watch": "vitest --watch",
		"test:browser": "echo 'Open test/*.html files in browser for manual UI testing'",
		"lint": "eslint . --ext .ts,.tsx,.js,.jsx",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"type-check": "tsc --noEmit",
		"ci": "npm run build && npm run lint && npm run format:check && npm run type-check && npm run test:run",
		"quality-report": "npm run ci",
		"prepublishOnly": "npm run clean && npm run build",
		"clean": "rm -rf dist"
	},
	"keywords": [
		"education",
		"code-study",
		"typescript",
		"jsx",
		"preact",
		"web-components",
		"functional-programming",
		"code-visualization",
		"programming-education",
		"code-transformation",
		"interactive-learning",
		"developer-tools",
		"authoring-tools",
		"learning-environments",
		"educational-toolkit"
	],
	"author": "Study Lenses Contributors",
	"license": "MIT",
	"homepage": "https://github.com/username/study-lenses-wc-kit#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/username/study-lenses-wc-kit.git"
	},
	"bugs": {
		"url": "https://github.com/username/study-lenses-wc-kit/issues"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"dependencies": {
		"preact": "^10.0.0",
		"prettier": "^3.0.0"
	},
	"devDependencies": {
		"typescript": "^5.0.0",
		"eslint": "^9.35.0",
		"prettier": "^3.0.0",
		"@types/node": "^20.0.0",
		"@types/preact": "^10.0.0",
		"@preact/preset-vite": "^2.0.0",
		"@jest/globals": "^29.0.0",
		"vitest": "^1.0.0",
		"@vitest/ui": "^1.0.0",
		"jsdom": "^22.0.0",
		"happy-dom": "^12.0.0"
	},
	"peerDependencies": {},
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/username"
	}
}
